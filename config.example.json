{
  "chats": ["you", "you,them"],
  "init": {
    "type": "init",
    "username": "yourusername",
    "paperkey": "enter your paper key"
  },
  "watcher": {
    "enabled": false,
    "timeout": 20
  },
  "eol": "\n",
  "attachments": {
    "addStub": true
  },
  "elasticsearch": {
    "enabled": false,
    "indexPattern": "keybase_$channelname$",
    "config": {
      "host": "localhost:9200",
      "log": "info"
    }
  },
  "jsonl": {
    "enabled": true,
    "file": "export.jsonl"
  }
}
