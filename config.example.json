{
  "chats": ["you", "you,them"],
  "init": {
    "type": "initFromRunningService"
  },
  "watcher": {
    "enabled": true,
    "timeout": 20
  },
  "eol": "\n",
  "elasticsearch": {
    "enabled": true,
    "indexPattern": "keybase_$channelname$",
    "config": {
      "host": "localhost:9200",
      "log": "info"
    }
  },
  "jsonl": {
    "enabled": false,
    "file": "export.jsonl"
  }
}
