{
  "name": "keybase-export",
  "version": "0.2.0",
  "description": "A tool to export keybase chats",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/",
    "flow": "flow check",
    "lint": "eslint . --max-warnings 0",
    "types": "flow-copy-source src dist",
    "build": "babel src/ -d dist/",
    "test": "npm run flow && npm run lint",
    "prepare": "npm run clean && npm run build",
    "prepack": "npm test",
    "start": "npm run build && node dist"
  },
  "files": [
    ".flowconfig",
    "dist",
    "bin",
    "config.example.json",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@hapi/joi": "^14.5.0",
    "debug": "^3.2.6",
    "elasticsearch": "^15.3.1",
    "keybase-bot": "^2.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-flow": "^7.0.0",
    "@bannerets/eslint-config-flow": "^0.5.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.9.0",
    "flow-bin": "^0.76.0",
    "flow-copy-source": "^1.3.0",
    "rimraf": "^2.6.2"
  },
  "bin": {
    "keybase-export": "bin/keybase-export"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "author": "Bannerets <comonoid@protonmail.com>",
  "license": "MIT",
  "keywords": [
    "keybase"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bannerets/keybase-export.git"
  },
  "bugs": {
    "url": "https://github.com/Bannerets/keybase-export/issues"
  },
  "homepage": "https://github.com/Bannerets/keybase-export#readme"
}
